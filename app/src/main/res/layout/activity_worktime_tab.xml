<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/scrollView1"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

   	<LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dip"
        android:orientation="vertical"
        android:padding="2dp"
        android:showDividers="end|beginning|middle"
        android:divider="@drawable/divider">
        
   	                
   	    <LinearLayout
   	        android:layout_width="match_parent"
   	        android:layout_height="wrap_content"
   	        android:orientation="horizontal">
        
		     <TextView
	            android:id="@+id/tvTimeIn"
	            android:layout_width="0dip"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center"
	            android:layout_weight=".7"
	            android:textAppearance="?android:attr/textAppearanceLarge"
	            android:textColor="@color/blue"
	            android:textSize="50sp"
	            android:onClick="onClick"
	            android:clickable="true"/>

		    <ImageButton
		        android:id="@+id/btnSelTimeIn"
		        style="@style/imageButtonStyle"/>
		    
		    </LinearLayout>

   	    <LinearLayout
   	        android:layout_width="match_parent"
   	        android:layout_height="wrap_content"
   	        android:orientation="horizontal" >
        
		    <TextView
	            android:id="@+id/tvBreakIn"
	            android:layout_width="0dip"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center"
	            android:layout_weight=".7"
	            android:textAppearance="?android:attr/textAppearanceLarge"
	            android:textColor="@color/blue"
	            android:textSize="50sp"
	            android:onClick="onClick"
	            android:clickable="true" />
    
		    <ImageButton
		        android:id="@+id/btnSelBreakIn"
		        style="@style/imageButtonStyle" />
		    
		    </LinearLayout>
		    
   	    <LinearLayout
   	        android:layout_width="match_parent"
   	        android:layout_height="wrap_content"
   	        android:orientation="horizontal" >
        
		   <TextView
	            android:id="@+id/tvBreakOut"
	            android:layout_width="0dip"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center"
	            android:layout_weight=".7"
	            android:textAppearance="?android:attr/textAppearanceLarge"
	            android:textColor="@color/blue"
	            android:textSize="50sp"
	            android:clickable="true"
	            android:onClick="onClick" />
    
		    <ImageButton
		        android:id="@+id/btnSelBreakOut"
		        style="@style/imageButtonStyle" />
		    
		    </LinearLayout>
		    
   	    <LinearLayout
   	        android:layout_width="match_parent"
   	        android:layout_height="wrap_content"
   	        android:orientation="horizontal">
        
		    <TextView
	            android:id="@+id/tvTimeOut"
	            android:layout_width="0dip"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center"
	            android:layout_weight=".7"
	            android:textAppearance="?android:attr/textAppearanceLarge"
	            android:textColor="@color/blue"
	            android:textSize="50sp"
	            android:onClick="onClick"
	            android:clickable="true" />
    
		    <ImageButton
		        android:id="@+id/btnSelTimeOut"
		        style="@style/imageButtonStyle"/>
		    
		    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:showDividers="middle"
        android:divider="@drawable/divider">
    
    
        <Button
